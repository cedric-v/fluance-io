{
  "functions": {
    "source": "functions",
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint"
    ]
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "hosting": {
    "public": "_site",
    "cleanUrls": true,
    "trailingSlash": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/firebase-web-authn-api",
        "function": "ext-firebase-web-authn-fu06-api"
      }
    ]
  },
  "extensions": {
    "firebase-web-authn-fu06": "gavinsawyer/firebase-web-authn@10.4.4"
  }
}