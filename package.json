{
  "name": "fluance-io",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel dev:*",
    "dev:11ty": "cross-env ELEVENTY_ENV=dev eleventy --serve",
    "dev:css": "npx tailwindcss -i ./src/assets/css/styles.css -o ./_site/assets/css/styles.css --watch",
    "build": "cross-env ELEVENTY_ENV=prod npm-run-all build:css build:11ty",
    "build:11ty": "eleventy",
    "build:css": "npx tailwindcss -i ./src/assets/css/styles.css -o ./_site/assets/css/styles.css --minify"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eleventy-plugin-i18n": "^0.1.3",
    "eleventy-plugin-seo": "^0.5.2",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.22",
    "html-minifier-next": "^4.6.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.13"
  }
}